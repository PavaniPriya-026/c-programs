import java.util.Scanner;
public class Solution {
    public static void main(String[] argh){
        Scanner sc = new Scanner(System.in);
        System.out.println("================================");
        for (int i = 0; i < 3; i++){
            String string = sc.next();
            int j = sc.nextInt();
            System.out.printf("%-15s%03d%n",string,j);
        }
        System.out.println("================================");
    }
}
